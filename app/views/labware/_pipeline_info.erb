<div class="mt-3 ml-3 mr-3">
  <strong><%=  @pipeline_info.pipeline_names %></strong> »

  <% if @pipeline_info.grandparents.any? %>
    ... »
  <% end %>

  <% if @pipeline_info.parents.any? %>
    <%= @pipeline_info.previous_purposes %> »
  <% end %>

  <strong><%= @labware.purpose.name %></strong>

  <% if @pipeline_info.children.any? %>
    <span class="text-muted">» <%= @pipeline_info.next_purposes %></span>
  <% end %>

  <% if @pipeline_info.grandchildren.any? %>
    <span class="text-muted">» ...</span>
  <% end %>
</div>
