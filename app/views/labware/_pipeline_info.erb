<div class="mt-3 ml-3 mr-3">
  <strong>
    <% if @pipeline_info.pipeline_group_name%>
      <%= link_to @pipeline_info.pipeline_group_name, @pipeline_info.graph_path(pipelines_path), target: '_blank', rel: 'noopener' %>
    <% else %>
      <%= @pipeline_info.pipeline_group_names %>
    <% end %>
  </strong> |

  <% if @pipeline_info.great_grandparent_purposes? %>
    ... »
  <% end %>

  <% if @pipeline_info.grandparent_purposes.present? %>
    <code><%= @pipeline_info.grandparent_purposes %></code> »
  <% end %>

  <% if @pipeline_info.parent_purposes.present? %>
    <code><%= @pipeline_info.parent_purposes %></code> »
  <% end %>

  <code><strong><u><%= @labware.purpose.name %></u></strong></code>

  <% if @pipeline_info.child_purposes.present? %>
    <code><span class="text-muted">» <%= @pipeline_info.child_purposes %></span></code>
  <% end %>

  <% if @pipeline_info.grandchild_purposes? %>
    <code><span class="text-muted">» ...</span></code>
  <% end %>
</div>
