<%= form_tag new_illumina_b_plate_child_path(presenter.plate.uuid), :method => 'get' do %>
  <%= hidden_field_tag :plate_purpose_uuid, presenter.child_purposes.first.uuid %>
  <%= submit_tag "Create #{presenter.child_purposes.first.name} plate", 
    :name           => nil,
    :class          => "submit",
    :'data-icon'    => "arrow-r",
    :'data-iconpos' => "right",
    :'data-theme'   => "b"
  %>
<% end %>

