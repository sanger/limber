<%
  rows_array = []
  @plate.wells_in_columns.each do |well|
    well.each_source_barcode_and_location_from_transfer_requests do |src_barcode, src_location|
      rows_array << [
                      src_barcode,
                      src_location,
                      @plate.labware_barcode.human,
                      well.position['name'],
                      10.00
                    ]
    end
  end
%>
<% rows_array.sort_by{ |a| WellHelpers.well_coordinate(a[1]) }.each do |row| %>
<%= CSV.generate_line row, row_sep: "" %>
<% end %>
