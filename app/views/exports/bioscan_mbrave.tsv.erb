<%= 
    rows = [['Forward Labels', 'Reverse Labels', 'Label', 'Group', 'UMI plate ID', 'Sample Plate ID']]
    @tube.aliquots.each do |aliquot| 
        rows << [aliquot.tag_index, aliquot.tag2_index, aliquot.sample.sample_metadata.supplier_name, nil, nil, nil]
    end

    rows.map(&:to_csv).join
%>